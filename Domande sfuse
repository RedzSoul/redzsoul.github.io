Cosa stampa a video 2?

int a=3;

void ALI(int ladri, int &ladroni) {

            ladroni=ladri*2*a++;

           

}

void BABA(int ladroni, int &ladri) {

               ladri=ladroni*3;

ladroni=++a;     

}

int main() {

            int b,c;

               a=5;

             b=6;

               c=7;

ALI(a,b);

BABA(b,c);

cout<<a<<endl;

cout<<b<<endl;

cout<<c<<endl;

return(0);

}

